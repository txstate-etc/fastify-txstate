    "@txstate-mws/fastify-shared": "^1.0.9",
    "@types/ua-parser-js": ">=0.7.39",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.0",
    "fastify": "^4.9.2",
    "fastify-plugin": "^4.5.1",
    "http-status-codes": "^2.1.4",
    "jose": "^6.0.0",
    "txstate-utils": "^1.9.5",
    "ua-parser-js": "^1.0.37"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^10.0.0",
    "@types/node": "^22.0.0",
    "axios": "^1.6.8",
    "chai": "^4.2.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "json-schema-to-ts": "^3.0.1",
    "mocha": "^10.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/txstate-etc/fastify-txstate.git"
  },
