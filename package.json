{
  "name": "fastify-txstate",
  "version": "2.3.0",
  "description": "A small wrapper for fastify providing a set of common conventions & utility functions we use.",
  "exports": {
    "require": "./lib/index.js",
    "import": "./lib-esm/index.js"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rm -rf lib && tsc",
    "test": "./test.sh",
    "mocha": "TS_NODE_PROJECT=test/tsconfig.json mocha -r ts-node/register test/**/*.ts",
    "testserver": "ts-node-script testserver/index.ts"
  },
  "dependencies": {
    "fastify": "^3.7.0",
    "http-status-codes": "^2.1.4",
    "pino-pretty": "^6.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.2",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "axios": "^0.21.1",
    "chai": "^4.2.0",
    "eslint": "^7.11.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "mocha": "^9.1.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/txstate-etc/fastify-txstate.git"
  },
  "author": "Nick Wing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/txstate-etc/fastify-txstate/issues"
  },
  "homepage": "https://github.com/txstate-etc/fastify-txstate#readme",
  "files": [
    "lib/**/*",
    "lib-esm/**/*"
  ]
}
